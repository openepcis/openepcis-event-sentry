{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://openepcis.github.io/openepcis-event-sentry/json-schema-epcis-snippets/child-epcs-0.1.0.json",
    "title": "EPCIS childEPCs field",
    "description": "Specifies set of values for the childEPCs field, expressed as an array of unique EPC URIs.",
    "type": "object",
    "properties": {
	"childEPCs": {
	    "type": "array",
	    "items": {
		"$ref": "#/$defs/uri"
	    },
	    "uniqueItems": true
	}
    },
    "$defs": {
	"uri": {
	    "type": "string",
	    "format": "uri"
	}
    }
}
